#include <iostream>
#include <iomanip>

using namespace std;

int main() {
    int A,B;
    int a[3],b[3];
    int ca=0,cb=0;
    for(int i=0;i<3;i++){
        cin>>A;
        a[i]=A;
    }
    for(int i=0;i<3;i++){
        cin>>B;
        b[i]=B;
    }
    for(int i=0;i<3;i++){
        if(a[i]>b[i])
            ca++;
        if(b[i]>a[i])
            cb++;
    }
    cout<<ca<<" "<<cb;
}
