#include <iostream>
using namespace std;

int main() {
    int n,e;
    float cp,cn,cc;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>e;
        arr[i]=e;
    }
    for(int i=0;i<n;i++){
        if(arr[i]>0)
            cp++;
        if(arr[i]==0)
            cc++;
        if(arr[i]<0)
            cn++;
    }
    cout<<cp/n<<endl<<cn/n<<endl<<cc/n;
}
